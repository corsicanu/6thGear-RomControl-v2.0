<resources>


    <!--Contact info arrays-->

    <!--In this array you will put the icons for contact information,
    for example xda icon for your xda thread, site icon for your site, facebook icon and so on-->
    <array name="about_contact_us_drawables">
        <item>@drawable/tlg</item>
        <item>@drawable/paypal</item>
        <item>@drawable/xda</item>
    </array>

    <!--In this array you will put the titles for your contact info items. Keep the order consistent between all 3 arrays-->
    <string-array name="about_contact_us_text">
        <item>Community</item>
        <item>Support</item>
        <item>Forum</item>
    </string-array>

    <!--In this array you will put the titles for the URL strings that will open once a user clicked on the item-->
    <string-array name="about_contact_us_links" translatable="false">
        <item>https://t.me/hadesUpdates</item>
        <item>https://www.paypal.me/corsicanu</item>
        <item>http://bit.ly/xdacorsi</item>
    </string-array>



    <!--Team info arrays-->
    <array name="about_team_drawables">
        <item>@drawable/me</item>
        <item>@drawable/mesa</item>
        <item>@drawable/anan</item>
    </array>
    <string-array name="about_team_names">
        <item>@string/me</item>
        <item>@string/mesa</item>
        <item>@string/anan</item>
    </string-array>
    <string-array name="about_team_links" translatable="false">
        <item>http://forum.xda-developers.com/member.php?u=4658360</item>
        <item>http://forum.xda-developers.com/member.php?u=6911575</item>
        <item>http://forum.xda-developers.com/member.php?u=4637718</item>
    </string-array>



